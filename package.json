{
  "author": "Eirik Albrigtsen <analsandblaster@gmail.com>",
  "name": "interlude",
  "description": "Functional JavaScript library",
  "version": "1.0.2",
  "repository": {
    "type": "git",
    "url": "clux/interlude"
  },
  "main": "interlude.js",
  "keywords": [
    "utility",
